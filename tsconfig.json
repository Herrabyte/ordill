    "compilerOptions": {
        "outDir": "./dist/",
        "module": "es6",
        "target": "es6",
        "lib": [
            "es2016"
        ],
        "moduleResolution": "node",
        "esModuleInterop": true,
        "strict": true,
        "jsx": "react",
        "jsxFactory": "h",
        "jsxFragmentFactory": "Fragment",
        "allowJs": true
    }
}
